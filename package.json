{
  "name": "nightscout-exporter",
  "version": "0.1.0",
  "description": "A utility to export Nightscout data as CSV or XLSX files.",
  "keywords": [
    "nightscout",
    "csv",
    "xlsx",
    "typescript"
  ],
  "author": "Jan Starzak <jan@superfly.tv>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstarpl/nightscout-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/jstarpl/nightscout-exporter/issues"
  },
  "homepage": "https://jstarpl.github.io/nightscout-exporter",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.6",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/underscore": "^1.9.2",
    "@wojtekmaj/react-datetimerange-picker": "^2.1.0",
    "file-saver": "^2.0.2",
    "gh-pages": "^2.0.1",
    "react-bootstrap": "^1.0.0-beta.9",
    "underscore": "^1.12.1",
    "url": "^0.11.0",
    "url-parse-lax": "^4.0.0",
    "xlsx": "^0.14.3"
  }
}
